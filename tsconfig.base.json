{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@tyrcord/feature/lazy/about": [
        "libs/feature/lazy/about/src/index.ts"
      ],
      "@tyrcord/feature/lazy/app-detail": [
        "libs/feature/lazy/app-detail/src/index.ts"
      ],
      "@tyrcord/feature/lazy/app-privacy-policy": [
        "libs/feature/lazy/app-privacy-policy/src/index.ts"
      ],
      "@tyrcord/feature/lazy/app-terms-of-service": [
        "libs/feature/lazy/app-terms-of-service/src/index.ts"
      ],
      "@tyrcord/feature/lazy/apps": [
        "libs/feature/lazy/apps/src/index.ts"
      ],
      "@tyrcord/feature/lazy/contact": [
        "libs/feature/lazy/contact/src/index.ts"
      ],
      "@tyrcord/feature/lazy/home": [
        "libs/feature/lazy/home/src/index.ts"
      ],
      "@tyrcord/feature/lazy/secret": [
        "libs/feature/lazy/secret/src/index.ts"
      ],
      "@tyrcord/feature/shared/localization": [
        "libs/feature/shared/localization/src/index.ts"
      ],
      "@tyrcord/feature/shared/logic/models": [
        "libs/feature/shared/logic/models/src/index.ts"
      ],
      "@tyrcord/feature/shared/logic/persistance": [
        "libs/feature/shared/logic/persistance/src/index.ts"
      ],
      "@tyrcord/feature/shared/logic/services": [
        "libs/feature/shared/logic/services/src/index.ts"
      ],
      "@tyrcord/testing": [
        "libs/testing/src/index.ts"
      ],
      "@tyrcord/ui/component": [
        "libs/ui/component/src/index.ts"
      ],
      "@tyrcord/ui/core": [
        "libs/ui/core/src/index.ts"
      ],
      "@tyrcord/ui/layout": [
        "libs/ui/layout/src/index.ts"
      ],
      "@tyrcord/ui/typography": [
        "libs/ui/typography/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
